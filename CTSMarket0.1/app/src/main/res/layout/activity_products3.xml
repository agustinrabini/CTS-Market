<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffff"
    android:fitsSystemWindows="true"
    tools:context=".activities.productsClasses.ProductsActivity3">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline344"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline346"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.05" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline348"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.09" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline347"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline349"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.91" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline345"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.07" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline350"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.09" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline351"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.13" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline352"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.15" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline353"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.2" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline355"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.45" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline357"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.9" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline359"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.95" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline358"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/ligthBlue"
        android:elevation="35dp"
        android:fontFamily="@font/quicksand_semibold"
        android:gravity="center"
        android:text="INFORMACIÓN DE ENVÍO"
        android:textColor="#000000"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline345"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_user_info4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="15dp"
        android:fontFamily="@font/quicksand_regular"
        android:gravity="left"
        android:text=""
        android:paddingTop="5dp"
        android:textColor="#000000"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline355"
        app:layout_constraintEnd_toStartOf="@+id/guideline349"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline348"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="5dp"
        android:layout_margin="5dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline355"
        app:layout_constraintEnd_toStartOf="@+id/guideline347"
        app:layout_constraintStart_toStartOf="@+id/guideline346"
        app:layout_constraintTop_toTopOf="@+id/guideline353"
        app:srcCompat="@drawable/background_general6" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="3dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline355"
        app:layout_constraintEnd_toStartOf="@+id/guideline347"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/guideline346"
        app:layout_constraintTop_toTopOf="@+id/guideline352"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/background_general3" />

    <TextView
        android:id="@+id/tv_location4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="15dp"
        android:fontFamily="@font/quicksand_regular"
        android:gravity="center_vertical"
        android:text="Datos del Usuario"
        android:textColor="#000000"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline353"
        app:layout_constraintEnd_toStartOf="@+id/guideline344"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline348"
        app:layout_constraintTop_toTopOf="@+id/guideline352" />

    <TextView
        android:id="@+id/tv_change_user_info4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="25dp"
        android:fontFamily="@font/quicksandbold"
        android:gravity="center_vertical"
        android:text="Editar infromación"
        android:textColor="@color/yellow"
        android:textSize="15dp"
        android:paddingTop="5dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/guideline358"
        app:layout_constraintEnd_toStartOf="@+id/guideline349"
        app:layout_constraintStart_toStartOf="@+id/guideline348"
        app:layout_constraintTop_toTopOf="@+id/guideline355" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fontFamily="@font/quicksand_medium"
        android:gravity="center"
        android:text="Ingrese los datos requeridos"
        android:textColor="@color/ligthBlue"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline351"
        app:layout_constraintEnd_toStartOf="@+id/guideline349"
        app:layout_constraintStart_toStartOf="@+id/guideline348"
        app:layout_constraintTop_toTopOf="@+id/guideline350" />

    <Button
        android:id="@+id/btn_buy4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/background_general3"
        android:fontFamily="@font/quicksand_semibold"
        android:gravity="center"
        android:text="CONTINUAR"
        android:textColor="#ffffff"
        android:textSize="12dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/guideline359"
        app:layout_constraintEnd_toStartOf="@+id/guideline347"
        app:layout_constraintStart_toStartOf="@+id/guideline346"
        app:layout_constraintTop_toTopOf="@+id/guideline357" />

    <TextView
        android:id="@+id/tv_need_to_add_user_info4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="15dp"
        android:fontFamily="@font/quicksand_regular"
        android:gravity="center"
        android:text="DEBE AGREGAR INFORMACIÓN PARA CONTINUAR"
        android:textColor="#d50000"
        android:textSize="12dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/guideline359"
        app:layout_constraintEnd_toStartOf="@+id/guideline347"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline346"
        app:layout_constraintTop_toTopOf="@+id/guideline357"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/iv_need_to_add_user_info4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/guideline359"
        app:layout_constraintEnd_toStartOf="@+id/guideline347"
        app:layout_constraintStart_toStartOf="@+id/guideline346"
        app:layout_constraintTop_toTopOf="@+id/guideline357"
        app:srcCompat="@drawable/btnprevio_compra" />

</androidx.constraintlayout.widget.ConstraintLayout>